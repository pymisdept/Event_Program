<script type="text/javascript" src="@Url.Content("~")Scripts/marquee/jquery.marquee.min.js"></script>
<script type="text/javascript" src="@Url.Content("~")Scripts/lightbox/js/lightbox.js"></script>
<link rel="stylesheet" href="@Url.Content("~")Scripts/lightbox/css/lightbox.css" type="text/css" media="screen" />
	
<script>
		$(document).ready(function ($) {
			var today = new Date();
			var weekday = new Array('Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday');
			var months = new Array('January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December');
	
			$('#pyedate').html(today.getDate());
			$('#pyemonth').html(months[today.getMonth()]);
			$('#pyeweek').html(weekday[today.getDay()]);
			
			var hkourl= 'https://data.weather.gov.hk/weatherAPI/opendata/weather.php?dataType=rhrread&lang=en';
			$.ajax({
				type: "GET",
				url: hkourl,
				dataType: 'json'
			}).done(function (data) {
				$('#w_d_v').html(data.temperature.data[1].value+"&#8451;");			
				$('#w_h_v').html(data.humidity.data[0].value+"%");			
			});
			
			$.getJSON( "@Url.Content("~")Content/PYNews/pynews.json?nocache=202109101034", function(obj) {
			  var data = obj.links;
			  $.each(data, function(key, value) {						
					$('#newsfeed').append('<a href="' + value.url + '" target="_blank">' + value.title + '</a>' + "&emsp;&emsp;&emsp;&emsp;");
			  });
			 });
		 
			 
			var url2 = 'https://rthk.hk/rthk/news/rss/c_expressnews_clocal.xml';
			$.ajax({
			  type: 'GET',
			  url: "https://api.rss2json.com/v1/api.json?rss_url=" + url2,
			  dataType: 'jsonp',
			  success: function(data) {
			  
				$.each(data.items, function( k, v ) {	
					$('#newsfeed').append(v.title + "&emsp;&emsp;&emsp;&emsp;");					
				});		
				
				$('.marquee').marquee({
						speed: 60,
						delayBeforeStart: 500,
						direction: 'left',
						pauseOnHover: true
				});
			  }
			});
					
			lightbox.option({
			  'resizeDuration': 200,
			  'wrapAround': true
			})
		});	
</script>
