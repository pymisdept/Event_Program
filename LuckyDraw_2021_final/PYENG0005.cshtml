@using System.Web.Mvc.Html
@using Projection.Business.Core
@using System.Web.Mvc.Html
@using Business
@using System.Linq

@model UI.ViewModels.PYENG0005ViewModel



@{
    Layout = "~/Views/Shared/_Shell.cshtml";
}

@section pageStyles
{
    @Html.Partial("_PageCss")
    @Styles.Render("~/css/documentoption")
		
    <style>
        [name="pyengSideBar"] {
            padding-top: 100px !important;
        }

            [name="pyengSideBar"] [name="closebtn"] {
                margin-top: 85px !important;
            }

        #pageContents {
            margin-top: 70px !important;
            <!-- height: calc(100vh - 50px); -->
        }

        .centre.pageWidth span {
            margin-left: 10px;
            vertical-align: top;

        }

        .centre.pageWidth .scenario-page-bg {
            padding: 10px 5px 5px 5px;
            cursor: pointer;
        }

            .centre.pageWidth .scenario-page-bg .imgIconHeader {
                height: 17px;
                width: 17px;
            }

            .centre.pageWidth .scenario-page-bg span {
                font-size: 14pt;
            }
			
			.centre.pageWidth{              
			  padding-left: 80px;
			  padding-right: 40px;
			  width: 90vw;
			  max-width: 90vw !important;
            }

        .pyImage {
            max-height: 100%;
			width: 100%;
            max-width: 1920px;
            cursor: default;
        }

        /*.centre.pageWidth .imgThumbnail {
            max-height: 150px;            
            display: inline-block;
            height: auto;
            width: auto;
        }*/

        .centre.pageWidth .imgThumbnail {
            height: 150px;            
        }

        .centre.pageWidth .sam.table {
            width: 100%;
        }

            .centre.pageWidth .sam.table .docNo {
                width: 255px;
            }

            .centre.pageWidth .sam.table .docSubject {
            }

            .centre.pageWidth .sam.table .docDate {
                width: 15%;
                text-align: center;
            }

            .centre.pageWidth .sam.table .docAttachments {
                width: 10%;
                text-align: center;
            }

            .centre.pageWidth .sam.table thead th {
                position: sticky;
                top: 0;
                text-align: center;
            }

            .centre.pageWidth .sam.table tbody td {
                font-size: 12pt;
            }

        .centre.pageWidth .tableScroll {
            /*max-height: 200px;*/
            overflow: hidden;
        }

        .centre.pageWidth .photoTable {
            height: 180px;
            overflow-x: auto;
        }

            .centre.pageWidth .photoTable .ui.very.basic.table {
                width: 100%;
            }

                .centre.pageWidth .photoTable .ui.very.basic.table td {
                    text-align: center;
                    vertical-align: middle;
                }

        .tsSummary {
            margin-top: 10px;
            width: 100%;
            text-align: center
        }

            .tsSummary thead th {
                background-color: #EFEFEF;
            }

            .tsSummary tbody tr {
                margin-top: 5px;
            }

        .value {
            font-size: 2em !important;
        }

        .lbl.stat.text {
            font-size: 12px;
        }

        .ui.column.center.aligned.grid.tsTitle {
            text-align: center;
            background-color: lightgrey;
            padding: 5px !important;
            margin-top: 0px !important;
            font-weight: bold;
        }

        .grey-background {
            height: calc(100vh - 85px);
        }

        .loadMore {
            width: 100%;
            text-align: right;
            font-weight: bold;
            padding-right: 10px;
        }

        .actionBar .project-filter {
            float: right;
            margin-top: 7px !important;
            <!-- margin-right: 20px; -->
            width: 250px;
        }
		
		.shortcutbar {
			display: inline-block;
		}

        @@media only screen and (max-width: 1366px) {
            .centre.pageWidth {
                width: 100vw !important;
				max-width: 100vw !important;
            }
			
			.shortcutbar {
				display: none;
			}
        }
		
		
		/* Slideshow container */
		.slideshow-container {		  
		  position: relative;
		  margin: auto;
		}

		/* Hide the images by default */
		.mySlides {
		  display: none;
		}

		/* Next & previous buttons */
		.prev, .next {
		  cursor: pointer;
		  position: absolute;
		  top: 50%;
		  width: auto;
		  margin-top: -22px;
		  padding: 16px;
		  color: white;
		  font-weight: bold;
		  font-size: 18px;
		  transition: 0.6s ease;
		  border-radius: 0 3px 3px 0;
		  user-select: none;
		}

		/* Position the "next button" to the right */
		.next {
		  right: 0;
		  border-radius: 3px 0 0 3px;
		}

		/* On hover, add a black background color with a little bit see-through */
		.prev:hover, .next:hover {
		  background-color: rgba(0,0,0,0.8);
		}
		
		/* The dots/bullets/indicators */
		.dot {
		  cursor: pointer;
		  height: 15px;
		  width: 15px;
		  margin: 0 2px;
		  background-color: #bbb;
		  border-radius: 50%;
		  display: inline-block;
		  transition: background-color 0.6s ease;
		}
		
		.dot:hover {
		  background-color: #717171;
		}

		.slideractive {
		  background-color: #717171;
		}
		
		.card {
